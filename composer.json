{
    "name": "rebolon/symfony-serializer-samples",
    "type": "library",
    "description": "A simple project to explain how to use Symfony Serializer, in real life ;)",
    "keywords": ["SYMFONY", "SERIALIZER", "JSON"],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "api-platform/core": "^2.2.4",
        "doctrine/annotations": "^1.6",
        "symfony/property-access": "^4.0.6",
        "symfony/property-info": "^4.1",
        "symfony/serializer": "^4.0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "roave/security-advisories": "dev-master",
        "sensiolabs/security-checker": "^4.1",
        "friendsofphp/php-cs-fixer": "^2.10",
        "squizlabs/PHP_CodeSniffer": "^3.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Rebolon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rebolon\\Tests\\": "tests/"
        }
    },
    "suggest": {
    }
}
